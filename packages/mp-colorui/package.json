{
  "name": "mp-colorui",
  "version": "1.0.0-beta.1",
  "description": "MP ColorUI 是一款基于 Taro 框架并且联合 Color-UI CSS 库开发的多端 UI 组件库（支持小程序端和H5端）",
  "keywords": [
    "taro",
    "mp-colorui",
    "colorui"
  ],
  "source": "src/index.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "types": "./@types/index.d.ts",
  "repository": "https://github.com/yinLiangDream/mp-colorui",
  "homepage": "https://yinliangdream.github.io/mp-colorui-doc/",
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "publishConfig": {
    "tag": "next"
  },
  "scripts": {
    "dev": "npm run dev:lib",
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
    "build": "npm run build:rollup && npm run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json",
    "build:rollup": "rollup --config ./config/rollup.config.js && node ./config/copy.js",
    "prepublishOnly": "npm run clean && npm run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf .temp dist lib coverage"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint --quiet --cache --fix",
      "git add"
    ]
  },
  "author": "liangyin",
  "license": "MIT",
  "dependencies": {
    "area-data": "^5.0.6",
    "dayjs": "^1.8.17",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-transform-react-jsx": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@tarojs/components": "^3.0.22",
    "@tarojs/mini-runner": "^3.0.22",
    "@tarojs/react": "^3.0.22",
    "@tarojs/runtime": "^3.0.22",
    "@tarojs/taro": "^3.0.22",
    "@types/classnames": "^2.2.9",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/sinon": "^5.0.7",
    "@types/webpack-env": "^1.13.9",
    "@vuepress/plugin-active-header-links": "^1.0.0-rc.1",
    "@vuepress/plugin-back-to-top": "^1.0.0-rc.1",
    "@vuepress/plugin-nprogress": "^1.0.0-rc.1",
    "@vuepress/plugin-search": "^1.0.0-rc.1",
    "axios": "^0.19.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
