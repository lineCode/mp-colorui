    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "cos-nodejs-sdk-v5": "^2.5.12",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "highlight": "^0.2.4",
